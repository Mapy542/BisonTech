\n// User defined function\nvoid myblockfunction_INIT();\n// User defined function\nvoid myblockfunction_Gyro_Telemetry();\n// User defined function\nvoid myblockfunction_turn_by_angle_turn_angle_speed(double myblockfunction_turn_by_angle_turn_angle_speed__turn_angle, double myblockfunction_turn_by_angle_turn_angle_speed__speed);\n\nint Brain_precision = 0, Console_precision = 0, Controller1_precision = 0;\n\nfloat myVariable, accelCheck, kill, desired_heading, correction;\n\nfloat Inputs[8];\nfloat velocity[20];\nfloat Cords[2];\nfloat avgacceleration[3];\n\nevent message1 = event();\nevent GetContAcceleration = event();\n\n// User defined function\nvoid myblockfunction_Set_x_y_heading(double myblockfunction_Set_x_y_heading__x, double myblockfunction_Set_x_y_heading__y, double myblockfunction_Set_x_y_heading__heading) {\n  Gryo.setHeading(myblockfunction_Set_x_y_heading__heading, degrees);\n}\n\n// User defined function\nvoid myblockfunction_INIT() {\n  accelCheck = 0.0;\n  myVariable = 0.0;\n  kill = 0.0;\n  Gryo.startCalibration();\n  while (Gryo.isCalibrating()) { task::sleep(50); }\n  Gryo.setHeading(0.0, degrees);\n  Gryo.setRotation(0.0, degrees);\n  LB.stop();\n  RB.stop();\n  LF.stop();\n  LR.stop();\n  RightVeyor.stop();\n  RightVeyor.stop();\n  Cam.stop();\n  whack.stop();\n  LB.setStopping(brake);\n  RB.setStopping(brake);\n  LF.setStopping(brake);\n  LR.setStopping(brake);\n  LeftVeyor.setStopping(brake);\n  RightVeyor.setStopping(brake);\n  Cam.setStopping(hold);\n  whack.setStopping(hold);\n}\n\n// Used to find the format string for printing numbers with the\n// desired number of decimal places\nconst char* printToBrain_numberFormat() {\n  // look at the current precision setting to find the format string\n  switch(Brain_precision){\n    case 0:  return \"%.0f\"; // 0 decimal places (1)\n    case 1:  return \"%.1f\"; // 1 decimal place  (0.1)\n    case 2:  return \"%.2f\"; // 2 decimal places (0.01)\n    case 3:  return \"%.3f\"; // 3 decimal places (0.001)\n    default: return \"%f\"; // use the print system default for everthing else\n  }\n}\n\n// User defined function\nvoid myblockfunction_Gyro_Telemetry() {\n  Brain.Screen.clearScreen();\n  Brain.Screen.setFont(mono20);\n  Brain.Screen.setCursor(1, 1);\n  Brain.Screen.print(\"Heading  \");\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Gryo.heading(degrees)));\n}\n\n// User defined function\nvoid myblockfunction_turn_by_angle_turn_angle_speed(double myblockfunction_turn_by_angle_turn_angle_speed__turn_angle, double myblockfunction_turn_by_angle_turn_angle_speed__speed) {\n  myblockfunction_Set_x_y_heading(1.0, 1.0, 180.0);\n  LB.setVelocity(myblockfunction_turn_by_angle_turn_angle_speed__speed, percent);\n  RB.setVelocity(myblockfunction_turn_by_angle_turn_angle_speed__speed, percent);\n  LF.setVelocity(myblockfunction_turn_by_angle_turn_angle_speed__speed, percent);\n  LR.setVelocity(myblockfunction_turn_by_angle_turn_angle_speed__speed, percent);\n  desired_heading = myblockfunction_turn_by_angle_turn_angle_speed__turn_angle + 180.0;\n  if (myblockfunction_turn_by_angle_turn_angle_speed__turn_angle > 0.0) {\n    while ((Gryo.heading(degrees) < desired_heading - 3.0)) {\n      if (Gryo.heading(degrees) > desired_heading - 45.0) {\n        LB.setVelocity((myblockfunction_turn_by_angle_turn_angle_speed__speed / 2.0), percent);\n        RB.setVelocity((myblockfunction_turn_by_angle_turn_angle_speed__speed / 2.0), percent);\n        LF.setVelocity((myblockfunction_turn_by_angle_turn_angle_speed__speed / 2.0), percent);\n        LR.setVelocity((myblockfunction_turn_by_angle_turn_angle_speed__speed / 2.0), percent);\n      }\n      RB.spin(reverse);\n      LR.spin(reverse);\n      LB.spin(forward);\n      LF.spin(forward);\n    wait(5, msec);\n    }\n    LB.stop();\n    RB.stop();\n    LF.stop();\n    LR.stop();\n  }\n  else {\n    while ((Gryo.heading(degrees) > desired_heading + 3.0)) {\n      if (Gryo.heading(degrees) < desired_heading + 45.0) {\n        LB.setVelocity((myblockfunction_turn_by_angle_turn_angle_speed__speed / 2.0), percent);\n        RB.setVelocity((myblockfunction_turn_by_angle_turn_angle_speed__speed / 2.0), percent);\n        LF.setVelocity((myblockfunction_turn_by_angle_turn_angle_speed__speed / 2.0), percent);\n        LR.setVelocity((myblockfunction_turn_by_angle_turn_angle_speed__speed / 2.0), percent);\n      }\n      RB.spin(forward);\n      LR.spin(forward);\n      LB.spin(reverse);\n      LF.spin(reverse);\n    wait(5, msec);\n    }\n    LB.stop();\n    RB.stop();\n    LF.stop();\n    LR.stop();\n  }\n}\n\n// \"when driver control\" hat block\nint ondriver_drivercontrol_0() {\n  myblockfunction_INIT();\n  myblockfunction_turn_by_angle_turn_angle_speed(-120.0, 10.0);\n  myblockfunction_Gyro_Telemetry();\n  myblockfunction_turn_by_angle_turn_angle_speed(-120.0, 10.0);\n  myblockfunction_Gyro_Telemetry();\n  myblockfunction_turn_by_angle_turn_angle_speed(-120.0, 10.0);\n  myblockfunction_Gyro_Telemetry();\n  wait(5.0, seconds);\n  return 0;\n}\n\nvoid VEXcode_driver_task() {\n  // Start the driver control tasks....\n  vex::task drive0(ondriver_drivercontrol_0);\n\n\n  while(Competition.isDriverControl() && Competition.isEnabled()) {this_thread::sleep_for(10);}\n  drive0.stop();\n  return;\n}\n\nvoid VEXcode_auton_task() {\n  // Start the auton control tasks....\n\n  while(Competition.isAutonomous() && Competition.isEnabled()) {this_thread::sleep_for(10);}\n\n  return;\n}\n\n\n\nint main() {\n  vex::competition::bStopTasksBetweenModes = false;\n  Competition.autonomous(VEXcode_auton_task);\n  Competition.drivercontrol(VEXcode_driver_task);\n\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n}"}